SYN: 1.00
VER: 2.04

DR NOVELL_NE1000
{
     DES: $NE1000_1
     HELP:     $NE1000_2
     PAR: 1.00
     FILE:     NE1000.LAN
     PROD:     EISA.ISA.871.1
;@# CLIENT: NE1000

	LIST Adapter_Bus_Type OPTIONAL
	{
		Description:	$NE1000_5
		Help:		$NE1000_6

		CDescription:	$NE1000_7
		Choice:		''
		CDescription:	$NE1000_8
		Choice:		'ISA'

		Default:	1
		OutputFormat:	'%s'
	}

     PR INT REQ
     {
          VAL: 2, 3, 4, 5
          DEF: 3
        }

     PR PORT REQ
     {
          VAL: 300, 320, 340, 360
          DEF: 300
          RES: 20
     }

     PR NODE
     {
     }

     FR FrameSelect
     {
          CH:  'Ethernet_802.3'
          CH:  'Ethernet_802.2'
          CH:  'Ethernet_SNAP'
          CH:  'Ethernet_II'
          DEF: 2
        }

     PR RETRIES
     {
          VAL: 0-255
          DEF: 5
     }


}

DR EAGLE_NE1000
{
     DES: $NE1000_3
     HELP:     $NE1000_4
     PAR: 1.00
     FILE:     NE1000.LAN
     PROD:     EISA.ISA.871.1
;@# CLIENT: NE1000

	LIST Adapter_Bus_Type OPTIONAL
	{
		Description:	$NE1000_5
		Help:		$NE1000_6

		CDescription:	$NE1000_7
		Choice:		''
		CDescription:	$NE1000_8
		Choice:		'ISA'

		Default:	1
		OutputFormat:	'%s'
	}

     PR INT REQ
     {
          VAL: 2, 3, 4, 5
          DEF: 3
        }

     PR PORT REQ
     {
          VAL: 300, 320, 340, 360
          DEF: 300
          RES: 20
     }

     PR NODE
     {
     }

     FR FrameSelect
     {
          CH:  'Ethernet_802.3'
          CH:  'Ethernet_802.2'
          CH:  'Ethernet_SNAP'
          CH:  'Ethernet_II'
          DEF: 2
        }

     PR RETRIES
     {
          VAL: 0-255
          DEF: 5
     }

}


