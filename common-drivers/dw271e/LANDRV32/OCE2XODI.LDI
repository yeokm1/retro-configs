VER:    1.00
SYN:    1.00

; Olicom 217x Ethernet server driver information file.

DRV OC217X
{
 PROD:  'PNPISA.INCLUDE.OLC.94A.5'
;@# Sprod: 'Olicom~ Ethernet ISA/IV Adapter (OC-217x)'
 DES:   $OCE2XODI_DES0
 HELP:  $OCE2XODI_DRVHLP0
 PAR:   2.00
 FILE:  OCE2XODI.LAN
; OF:    ETHERTSM.NLM,MSM.NLM,NBI.NLM

;@#  CLIENT: oce2xodi
;@#  ALIAS: oce2xodi

 PR SLOT OPT
  {
  HELP: $OCE2XODI_SLOTHLP
  }

 FR FRMTYPE
  {
  HELP: $OCE2XODI_FRMHLP
  CD:   $OCE2XODI_1
  CH:   'Ethernet_802.3'
  CD:   $OCE2XODI_2
  CH:   'Ethernet_802.2'
  CD:   $OCE2XODI_3
  CH:   'Ethernet_SNAP'
  CD:   $OCE2XODI_4
  CH:   'Ethernet_II'
  DEF:  1, 2
  }

 PR NODE
  {
  HELP: $OCE2XODI_NODEHLP
  }
}

; Olicom GoCard Ethernet server driver information file.

DRV OCGOCARD2220
{
