VER: 1.05
SYN: 1.00

DR NOVELL_CNE2
{
     DES:  $CNE2_1
     HELP:  $CNE2_2
     PAR: 2.00
     PROD:     'MCA.7154'
     FILE:     CNE2.LAN
;@#  Sprod: 'Novell ~ NE2 or compatible'
;@# CLIENT: CNE2
;@# ALIAS: NE2.LAN

     PR SLOT REQ
     {
          VAL: 1-65535
     }

     PR NODE
     {
     }

     FR FrameSelect
     {
          CH:  'Ethernet_802.2'
          CH:  'Ethernet_SNAP'
          CH:  'Ethernet_II'
          CH:  'Ethernet_802.3'
          DEF: 1,4
        }

     PR RETRIES
     {
          VAL: 0-255
          DEF: 5
     }


}

DR EAGLE_CNE2
{
     DES:  $CNE2_3
     HELP:  $CNE2_4
     PAR: 2.00
     PROD:     'MCA.7154'
     FILE:     CNE2.LAN
;@#  Sprod: 'EAGLE ~ NE2 or compatible'
;@# CLIENT: CNE2
;@# ALIAS: NE2.LAN

     PR SLOT REQ
     {
          VAL: 1-65535
     }

     PR NODE
     {
     }

     FR FrameSelect
     {
          CH:  'Ethernet_802.3'
          CH:  'Ethernet_802.2'
          CH:  'Ethernet_SNAP'
          CH:  'Ethernet_II'
          DEF: 2
        }

     PR RETRIES
     {
          VAL: 0-255
          DEF: 5
     }

}


