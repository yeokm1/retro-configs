DR PCnetISA
{
     DES: $PCNTNW_1
     HELP: $PCNTNW_2
     PAR: 1.00  
     FILE: PCNTNW.LAN
     PROD: EISA.NVL.090.1
;@# Sprod: 'AMD PCnet-ISA Ethernet card'

;@# CLIENT: PCNTNW.COM, PCNTNW.LAN, CPCNTNW.LAN, CNEAMD.LAN
;@# ALIAS:  PCNTNW.LAN, CPCNTNW.LAN, CNEAMD.LAN


     PR PCnetISA REQ
     {
        DES: $PCNTNW_8
        Help:   $PCNTNW_18
    TYP: STR
    DEF: 'ISA'
    VAL: 'ISA'
    OUT: '%s'
     }

     PR INT     REQ
     {
        TYP: HEX(3)
    VAL: 3, 4, 5, 9, A, B, C, F
    DEF: 3
        OUT: 'INT=%s'
     }

     PR PORT REQ
     {
        TYP: HEX(3)
    VAL: 300, 320, 340, 360, 200,220,240,260,280,2A0,2C0,2E0
    DEF: 300
    RES: 20
        OUT: 'PORT=%s'
     }

     PR DMA     REQ
     {
        TYP: HEX(3)
    VAL: 3, 5, 6, 7
    DEF: 5
        OUT: 'DMA=%s'
     }


     PR NODE
     {
     }

     FR FrameSelect
     {
    CH: 'Ethernet_802.2'
    CH: 'Ethernet_SNAP'
    CH: 'Ethernet_II'
    CH: 'Ethernet_802.3'
    DEF: 1,2,3,4
     }

     PR RETRIES
     {
    TYP: DEC
    VAL: 0-255
    DEF: 5
     }

}


