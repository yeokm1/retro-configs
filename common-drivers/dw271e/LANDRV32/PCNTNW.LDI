VER: 4.02
SYN: 1.00


DR PCnetFAST
{
     DES: $PCNTNW_5
     HELP: $PCNTNW_6
     PAR: 2.00  
     FILE: PCNTNW.LAN
     PROD: 'PCI.1022.2000.*.*.*'
;@# Sprod: 'AMD PCnet-FAST, PCnet-FAST+ Ethernet card'

;@# CLIENT: PCNTNW.COM, PCNTNW.LAN, CPCNTNW.LAN, CNEAMD.LAN
;@# ALIAS:  PCNTNW.LAN, CPCNTNW.LAN, CNEAMD.LAN

     PR PCI REQ
     {
    TYP: STR
    VAL: 'PCI'
    DEF: 'PCI'
    OUT: '%s'
     }
     
     LI PermaNet OPT
     {
        DES: "Enable PermaNet Server function?"
        Help: "PermaNet Server provides the failover function"
        CD:     "No"
        CH:     ''
        CD:     "Yes"
        CH:     ''
        DEF: 1
;                DEF: UND
     }
     
     PR PRIMARY_SLOT if (PermaNet == 2) REQ ELS HID
     {
        DES: "Please Enter the Primary Adapter Slot Number"
        Help: "Slot Number is a number between 1 - 65535 "
    TYP: DEC
    VAL: 1-65535
        OUT: 'PRIMARY=%s'
     }
     
     PR SECONDARY_SLOT if (PermaNet == 2) REQ ELS HID
     {
        DES: "Please Enter the Secondary Adapter Slot Number"
        Help: "Slot Number is a number between 1 - 65535 "
    TYP: DEC
    VAL: 1-65535
        OUT: 'SECONDARY=%s'
     }

     PR SLOT if (PermaNet == 2) HID ELS REQ
     {
    TYP: DEC
    VAL: 1-65535
     }

     PR NODE
     {
     }

     FR FrameSelect
     {
    CH: 'Ethernet_802.2'
    CH: 'Ethernet_SNAP'
    CH: 'Ethernet_II'
    CH: 'Ethernet_802.3'

    DEF: 1,2,3,4
     }

     PR RETRIES
     {
    TYP: DEC
    VAL: 0-255
    DEF: 5
     }
     

     LI LineSpeedMode OPT 
     {
        DES: $PCNTNW_22
        Help: $PCNTNW_23

        CD:     $PCNTNW_24
        CH:     'AUTO'
        CD:     $PCNTNW_25
        CH:     '100F'
        CD:     $PCNTNW_26
        CH:     '100H'
        CD:     $PCNTNW_27
        CH:     '10F'
        CD:     $PCNTNW_28
        CH:     '10H'
        DEF: 1        
;                DEF: UND
        OUT: 'LINESPEED=%s'
     }
     
     LI MPMODE OPT
     {
        DES: $PCNTNW_20
        Help: $PCNTNW_18

        CD:     $PCNTNW_15
        CH:     'OFF'
        CD:     $PCNTNW_16
        CH:     'ON'
        DEF: 1
;                DEF: UND
        OUT: 'MPMODE=%s'
     }

}

